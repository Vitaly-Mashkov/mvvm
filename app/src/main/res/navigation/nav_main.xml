<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_main"
    app:startDestination="@id/usersFrargment">

    <action
        android:id="@+id/action_global_to_system_dialog"
        app:destination="@+id/systemDF">
        <argument
            android:name="dialog"
            app:argType="com.mashkov.mvvm.models.SystemDialog" />
    </action>

    <dialog
        android:id="@+id/systemDF"
        android:name="com.mashkov.mvvm.fragments.dialogs.SystemDF"
        android:label="SystemDF">
        <argument
            android:name="dialog"
            app:argType="com.mashkov.mvvm.models.SystemDialog" />
    </dialog>


    <fragment
        android:id="@+id/usersFrargment"
        android:name="com.mashkov.mvvm.fragments.UsersF"
        android:label="UsersFrargment">
        <action
            android:id="@+id/action_usersFrargment_to_userInfoF"
            app:destination="@id/userInfoF">
            <argument
                android:name="login"
                app:argType="string" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/userInfoF"
        android:name="com.mashkov.mvvm.fragments.UserInfoF"
        android:label="UserInfoF">
        <argument
            android:name="login"
            app:argType="string" />
    </fragment>

</navigation>